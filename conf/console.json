{
	"http": {
		"port": 6074,
		"enableAutoPort": true,
		"ignoredInterfaces": ["docker0"]
	},
	"prefix": "tenured",
	"data": "/data/tenured",
	"logs": {
		"level": "error",
		"path": "/data/tenured/logs/console.log",
		"output": "stdout",
		"archive": false,
		"loggers": {
			"iris":"error"
		}
	},
	"registry": {
		"address": "consul://127.0.0.1:8500",
		"attributes": {
			"CheckType": "http"
		}
	}
}